{
  "name": "kedo-mind",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "description": "KedoMind",
  "author": "苏州蝌蚪文数字科技有限公司",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5174 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:pack": "npm run build && electron-builder --dir",
    "electron:dist": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.kedo.mind",
    "productName": "KedoMind",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "public/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/logo-mind_256.ico",
      "artifactName": "${productName}-win-${arch}-Setup.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@iktakahiro/markdown-it-katex": "^4.0.1",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@vueuse/core": "^13.0.0",
    "ajv": "^8.17.1",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.8.3",
    "crypto-js": "^4.2.0",
    "dexie": "^4.0.11",
    "echarts": "^5.6.0",
    "echarts-gl": "^2.0.9",
    "electron-log": "^5.4.1",
    "electron-updater": "^6.6.2",
    "file-saver": "^2.0.5",
    "highlight.js": "^11.11.1",
    "js-base64": "^3.7.7",
    "json-editor-vue": "^0.18.1",
    "katex": "^0.16.21",
    "lodash-es": "^4.17.21",
    "mammoth": "^1.9.0",
    "markdown-it": "^14.1.0",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "nanoid": "^5.1.5",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "pyodide": "^0.28.2",
    "qrcode": "^1.5.4",
    "terser": "^5.39.0",
    "vanilla-jsoneditor": "^3.8.0",
    "vue": "^3.5.13",
    "vue-i18n": "9",
    "vue-json-pretty": "^2.5.0",
    "vue-router": "^4.2.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "^32.0.0",
    "electron-builder": "^25.1.0",
    "sass": "^1.86.0",
    "vite": "^6.2.0",
    "wait-on": "^8.0.1"
  }
}
